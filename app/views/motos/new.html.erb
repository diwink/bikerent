<div class="container">
  <h1>Give us more information about your lovely motorcycle</h1>
  <%= simple_form_for [@user, @moto] do |f| %>
  <%= f.input :brand %>
  <%= f.input :model %>
  <%= f.input :address %>
  <%= f.input :description %>
  <%= f.input :price_per_day %>
  <%= f.input :photo, as: :file %>
  <%= f.submit "submit", class: "btn btn-primary"%>
  <% end %>
</div>
